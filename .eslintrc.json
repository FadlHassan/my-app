{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "eslint-plugin-absolute-imports"
    ],
    "rules": {
        "absolute-imports/only-absolute-imports": "error",
        "semi": 0
    }
}
